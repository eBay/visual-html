{
  "name": "visual-html",
  "description": "Visual regression testing without the flakiness.",
  "version": "2.1.1",
  "author": "Dylan Piercey <dpiercey@ebay.com>",
  "bugs": "https://github.com/eBay/visual-html/issues",
  "dependencies": {
    "specificity": "^0.4.1",
